@model Wolfpack.Web.Models.Account.NewUserVM

@{
    ViewBag.Title = "NewUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Welcome to WolfPack, please fill in your account information</h2>


@using (Html.BeginForm())
{

    @Html.Label("Username")
    @Html.EditorFor(m => m.UserName)

    @Html.Label("Firstname")
    @Html.EditorFor(m => m.FirstName)
 
    @Html.Label("Lastname")
    @Html.EditorFor(m => m.LastName)

    @Html.Label("Mailadress")
    @Html.EditorFor(m => m.MailAdress)

}
<input type="submit" />


